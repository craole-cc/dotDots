{
  "name": "dotDots",
  "git": {
    "user": "craole-cc",
    "email": "craole@example.com"
  },
  "options": {
    "tag": ">>= DOTS =<<",
    "verbosity": "Error",
    "verbosePreference": "SilentlyContinue",
    "debugPreference": "SilentlyContinue",
    "informationPreference": "SilentlyContinue",
    "warningPreference": "SilentlyContinue",
    "errorActionPreference": "Continue"
  },
  "experimental_features": {
    "enabled": [
      "PSFeedbackProvider"
    ]
  },
  "excludes": {
    "patterns": [
      "review",
      "tmp",
      "temp",
      "archive",
      "backup",
      "*.bac",
      "*.bak",
      "* copy*",
      "*.old",
      "Tests",
      "Test",
      "Configuration\\powershell"
    ]
  },
  "order_files": {
    "filenames": [
      ".dotsrc",
      ".dots.conf",
      ".dots.json",
      ".dots.toml",
      "dots.json",
      "dots.toml"
    ]
  },
  "includes": [
    {
      "path": "Bin\\powershell",
      "modules": [
        "Admin",
        "Components",
        "Types",
        "Output",
        "FileSystem",
        "Environment",
        "Applications"
      ]
    },
    {
      "path": "Configuration",
      "modules": [
        "scoop",
        "winget",
        "mise-en-place",
        "helix",
        "jujutsu",
        "rust",
        "neo4j",
        "neovim",
        "WindowsTerminal",
        "oh-my-posh"
      ]
    },
    {
      "path": "Tests\\powershell",
      "modules": []
    }
  ],
  "submodules": {
    "victus": {
      "path": "Configuration/hosts/Victus",
      "writable": true,
      "description": "Victus laptop NixOS configuration",
      "git": {
        "user": "Craole"
      }
    },
    "wallpapers": {
      "path": "Assets/Images/wallpaper",
      "writable": true,
      "description": "Wallpaper collection",
      "git": {
        "user": "Craole"
      }
    }
  }
}
